<%= render 'shared/errors',obj:@wallpaper %>
<div class="row">
	<div class="well col-md-8 col-md-offset-2">
<%= form_for @wallpaper do |f| %>
<%=f.label :name%>
<%=f.text_field :name%>
<%=f.label :summary%>
<%=f.text_field :summary%>
<%=f.label :description %>
<%=f.text_area :description,rows:10%>
<%=f.submit(@wallpaper.new_record? ? 'Create Wallpapers' : 'Edit wallpaper',class:"btn btn-success")%>
<%end%>
</div>
</div>